";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:6:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:20:"Board Meeting Report";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:34:"http://www.opensource.org/node/560";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:628:"The <a href="http://opensource.org/">Open Source Initiative</a> (OSI) Board meet this weekend in San Francisco for its annual face-to-face meeting (generously hosted by the Wikimedia Foundation). There were two significant topics on the agenda. First, we had to review the substantial number of nominations for the two Board seats that become vacant on March 31st when <a href="http://danesecooper.blogs.com/">Danese Cooper</a> and <a href="http://russnelson.com/">Russ Nelson</a> leave the Board due to term limits after a decade each of service.<p><a href="http://www.opensource.org/node/560" target="_blank">read more</a></p>";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:8:"comments";a:1:{i:0;a:5:{s:4:"data";s:43:"http://www.opensource.org/node/560#comments";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Thu, 17 Mar 2011 00:35:35 +0000";s:7:"attribs";a:0:{}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:32:"560 at http://www.opensource.org";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:25:"http://www.opensource.org";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}s:32:"http